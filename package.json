{
  "name": "console-feed-node-transform",
  "version": "3.2.3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "description": "Only Encode/Decode from console-feed",
  "scripts": {
    "build": "rm -rf lib && tsc -p ./tsconfig.build.json --declaration",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "release": "yarn test && yarn clean && yarn build && yarn publish && yarn clean",
    "precommit": "pretty-quick --staged",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^2.1.2",
    "pretty-quick": "^1.6.0",
    "ts-loader": "^3.5.0",
    "typescript": "^4.1.0-beta"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./scripts/test.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.spec.(ts|tsx|js)"
    ]
  },
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "keywords": [
    "devtools",
    "inspector",
    "object",
    "object-inspector",
    "react",
    "react-component",
    "reactjs",
    "table",
    "table-inspector",
    "table-view",
    "tableview",
    "tree",
    "tree-view",
    "treeview",
    "ui",
    "view"
  ],
  "homepage": "https://github.com/samdenty99/console-feed",
  "repository": {
    "type": "git",
    "url": "https://github.com/samdenty99/console-feed.git"
  },
  "bugs": {
    "url": "https://github.com/samdenty99/console-feed/issues"
  },
  "author": "Sam Denty <samddenty@gmail.com> (http://github.com/samdenty99)",
  "license": "MIT"
}
